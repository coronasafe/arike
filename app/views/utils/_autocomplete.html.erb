<!-- Because React is not integrated yet, The uniq_id helps mount several autocomplete components in the same page.-->
<%= content_tag(:div,"" ,id: uniq_id) %>
<%= javascript_pack_tag "autocomplete", 'data-turbolinks-track': "reload" %>
<script type="text/json" id="volunteer_list">
  <%= User.volunteers.select(:id, :full_name).to_json %>
</script>
<script>
  runReact("<%= uniq_id %>", "<%= api_endpoint %>", "<%= name %>", "<%= id %>", "<%= label %>", "<%= placeholder %>")
</script>
