<%=content_for(:tail) do %>
  <%= javascript_pack_tag 'PsychologicalReviewPack' %>
<% end %>

<script type="text/json" id="psychological-review-form-data">
  {
    "patient_worried": <%= info.patient_informed %>,
    "family_anxious": <%= info.family_anxious %>,
    "patient_feels": <%= info.patient_feels %>,
    "patient_depressed": <%= info.patient_depressed  %>,
    "patient_informed": "<%= info.patient_informed %>"
  }
</script>

<%= form_with url: new_visit_psychological_review_path(visit), method: :post do |form| %>
  <div id="psychological-review-form"></div>
<% end %>
